VPATH = .:..

# Standard base for g++.

CCC = g++
CCFLAGS = \
-DUNIX -O2 -DNDEBUG -I.. -I. -fexceptions -W
LIBS = -lm
LDFLAGS =

.SUFFIXES : .o .cc
.cc.o:; $(CCC) $(CCFLAGS) -c $<
#------------------------------------------------------------------------
$(TARGET): $(OBJS)
	-rm $@ >/dev/null 2>&1
	$(CCC) $(CCFLAGS) $(OBJS) -o $@ $(LIBS) $(LDFLAGS)
