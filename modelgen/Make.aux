#------------------------------------------------------------------------
# Part 3 of a Makefile
#------------------------------------------------------------------------
tags: $(HDRS) $(SRCS)
	etags --c++ $(HDRS) $(SRCS)
#------------------------------------------------------------------------
checkin:
	date "+#define PATCHLEVEL \"%Y.%m.%d RCS `cat rcsversion`\"" \
		>patchlev.h
	-ci -u`cat rcsversion` -m. -t/dev/null $(ALL)
	-ci -r`cat rcsversion` -m. -t/dev/null $(DELETED)
#------------------------------------------------------------------------
checkout:
	co $(ALL)
#------------------------------------------------------------------------
unclean:
	rm $(ALL)
#------------------------------------------------------------------------
backup:
	-mkdir BACKUP
	cp $(ALL) BACKUP
#------------------------------------------------------------------------
clean:
	-rm *.o
#------------------------------------------------------------------------
depend: $(SRCS) $(HDRS)
	$(CCC) -MM $(CCFLAGS) $(SRCS) > Make.depend
#-----------------------------------------------------------------------------
date:
	date "+#define PATCHLEVEL \"%Y.%m.%d RCS `cat rcsversion` +\"" \
		>patchlev.h
